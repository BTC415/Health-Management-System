{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {
     "endOfLine":"auto"
   }],
    "linebreak-style": 0,
    "no-console": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-unused-vars": "off"
  }
}
