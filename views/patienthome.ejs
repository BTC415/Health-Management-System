<div class="about-page pt_60 pb_30">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>Welcome to our patient portal </h1>
                <h3>Hi <%= data.user.name %></h3>      
                                
                <div class="faq-group headstyle mt-30">
                    <div id="accordion">
                        <div class="faq-item card">
                            <div class="faq-header" id="heading1">
                                <button class="faq-button " data-toggle="collapse" data-target="#collapse1" aria-expanded="true" aria-controls="collapse1"><i class="fa fa-caret-right"></i> Book An Appointment </button>
                            </div>
                            <div id="collapse1" class="collapse show" aria-labelledby="heading1" data-parent="#accordion">
                                <div class="faq-body">
                                    <p>
                                        <a href="/patienthome/bookappointment/<%= data.user._id%>" class="btn btn-success btn-large">Book an Appointment</a>  
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                            <div class="faq-header" id="heading2">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse2" aria-expanded="true" aria-controls="collapse2"><i class="fa fa-caret-right"></i>Book an Ambulance</button>
                            </div>

                            <div id="collapse2" class="collapse" aria-labelledby="heading2" data-parent="#accordion">
                                <div class="faq-body">
                                    <form action="/patienthome/<%= data.user._id %>/bookambulance" method="POST" onSubmit="alert('Your ambulance has been booked');" accept-charset="utf-8">
                                        <div class="form-row row mt_20 contact-form">
                                            <div class="form-group col-12">
                                                <div class="mb-3">
                                                    <p>
                                                        Please enter your address in the textarea
                                                    </p>
                                                    <textarea class="form-control" id="validationTextarea" placeholder="Address" name="address" autocomplete="off" required></textarea>
                                                    
                                                  </div>
                                            </div>
                                            <div class="form-group col-12">
                                                <label for="phonenum">Phone Number</label><br/>
                                                  <input id="phonenum" type="tel" class="form-control" placeholder="Phone Number (format:+91'xxxxxxxxxx')" name="phone" pattern="^\d{10}$" minlength="10" autocomplete="off" required>
                                              </div>                                                                                        
                                            <div class="form-group col-12">
                                                <button type="submit" class="btn btn-success btn-large" name="form_ambulance">Book an Ambulance</button>
                                            </div>
                                        </div>
                                    </form>

                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                            <div class="faq-header" id="heading3">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse3" aria-expanded="true" aria-controls="collapse3"><i class="fa fa-caret-right"></i>Current Appointment Details</button>
                            </div>

                            <div id="collapse3" class="collapse" aria-labelledby="heading3" data-parent="#accordion">
                                <div class="faq-body">
                                    <p> 
                                        <h2>Your Current Appointments with us</h2>
                                        <br>
                                        <ol>
                                            <% if(data.futureappointment != undefined && data.futureappointment.length > 0) { %>
                                                <% data.futureappointment.forEach(function(item){ %>
                                                <p>
                                                    <li>
                                                        <b>Appointment Date: <%= item.AppointmentDate %></b> 
                                                    </li>
                                                    <b>Appointment Time:</b> <%= item.AppointmentTime %> <br/>
                                                    <b>Disease: </b> <%= item.Disease %> <br/>
                                                    <b>Status: </b> <%= item.Status %>
                                                </p>
                                                <br/>
                                                <% }) %>
                                        </ol>
                                        <% }else{ %>
                                            <p>
                                                Currently there is no appointment !!!
                                            </p>
                                        <% } %>
                                    </p>                                     
                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                            <div class="faq-header" id="heading4">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse4" aria-expanded="true" aria-controls="collapse3"><i class="fa fa-caret-right"></i>Current Ambulance Details</button>
                            </div>
                            <div id="collapse4" class="collapse" aria-labelledby="heading4" data-parent="#accordion">
                                <div class="faq-body">
                                    <p> 
                                        <h2>Your Booked Ambulance Details </h2>
                                        <br/>
                                        <ol>
                                            <% if(data.ambulanceappointment != undefined && data.ambulanceappointment.length > 0 && Object.keys(data.ambulanceappointment[0]).length != 0) { %>
                                                <% data.ambulanceappointment.forEach(function(item){ %>
                                                    <li>
                                                        <b>Booking Date: </b><%= item.Date %> <br/>
                                                    </li>
                                                    <b>Booking Time: </b><%= item.Time %> <br/>                                          
                                                    <b>Address: </b><%= item.PatientAddress %> <br/>
                                                    <b>Booking Status: </b><%= item.BookingStatus %> <br/>
                                                    <b>Journey Status: </b><%= item.JourneyStatus %> <br/>
                                                    <% if(item.AmbulanceId != null){ %>
                                                        <b>Ambulance Details </b> <br/>
                                                        <b>Ambulance Name: </b><%= item.AmbulanceName %> <br/>
                                                        <b>Ambulance Phone Number: </b><%= item.AmbulancePhone %> <br/>
                                                        <b>Ambulance Vehicle Number: </b><%= item.AmbulanceVehicleNumber %> <br/>
                                                        <b>Ambulance Ambulance Type: </b><%= item.AmbulanceType %> <br/>
                                                        <img src="<%= item.AmbulanceImg %>" alt="Ambulance Image" width="100" heigth="100" >
                                                    <% } %>
                                                <% }) %>   
                                        </ol>                                                                             
                                        <% }else{ %>
                                        <p>
                                            Currently there is no ambulance booked by you !!!
                                        </p>
                                        <% } %>
                                    </p>                                     
                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                            <div class="faq-header" id="heading5">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse5" aria-expanded="true" aria-controls="collapse4"><i class="fa fa-caret-right"></i>Past Appointment Details</button>
                            </div>
                            <div id="collapse5" class="collapse" aria-labelledby="heading5" data-parent="#accordion">
                                <div class="faq-body">
                                    <p> 
                                        <h2>Your Past Appointments with us</h2>
                                        <br>
                                        <ol>
                                            <% if( data.pastappointment != undefined && data.pastappointment.length > 0) { %>
                                                <% data.pastappointment.forEach(function(item){ %>
                                                <p>
                                                    <li>
                                                        <b>Appointment Date: <%= item.AppointmentDate %></b> 
                                                    </li>
                                                    <b>Appointment Time:</b> <%= item.AppointmentTime %> <br/>
                                                    <b>Disease: </b> <%= item.Disease %> <br/>
                                                    <b>Status: </b> <%= item.Status %> <br/>
                                                    <% if(item.PrescriptionId){ %>
                                                        <b>Prescription: </b> <br/>
                                                        <b>Symptoms: </b><%= item.Symptoms %> <br/>
                                                        <b>Tests: </b><%= item.Tests %> <br/>
                                                        <b>Medicines: </b><%= item.Medicines %> <br/>
                                                        <b>Doctor Signature: </b> <br/>
                                                        <img src = "<%= item.DoctorSignature %>" width="100" heigth="100" alt="signature" > <br/>
                                                        
                                                    <% }else{ %>
                                                        Currently no prescription is uploaded
                                                    <% } %>
                                                </p>
                                                <br/>
                                                <% }) %>
                                        </ol>
                                        <% }else{ %>
                                            <p>
                                                Currently there is no appointment !!!
                                            </p>
                                        <% } %>
                                    </p>                                     
                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                            <div class="faq-header" id="heading6">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse6" aria-expanded="true" aria-controls="collapse5"><i class="fa fa-caret-right"></i> Update Credentials</button>
                            </div>

                            <div id="collapse6" class="collapse" aria-labelledby="heading6" data-parent="#accordion">
                                <div class="faq-body">
                                    <p>
                                        <a href="/patienthome/<%= data.user._id%>/edit" class="btn btn-success btn-large">Update Credentials</a>
                                    </p>                                   
                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
